from sklearn.feature_selection import VarianceThreshold
from sklearn.linear_model import LogisticRegression
from sklearn.feature_selection import SelectFromModel

# Variance filtering
vt = VarianceThreshold(threshold=1.0)
X_var = vt.fit_transform(X)

print("Genes after variance filter:", X_var.shape[1])

# LASSO selection
lasso = LogisticRegression(penalty="l1", solver="liblinear", max_iter=500)
lasso.fit(X_var, y)

selector = SelectFromModel(lasso, prefit=True)
X_selected = selector.transform(X_var)

print("Genes after LASSO:", X_selected.shape[1])
